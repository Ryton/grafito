do.import {grafito.art}

do [graph.create "sample2" [
    honorEnN: is 'Pos #[p:"noun"]
    honorEsN: is 'Pos #[p:"noun"]
    dishonorEnN: is 'Pos #[p:"noun"]

    tokenS: is 'Sense #[s:"token of praise or respect"]

    link (is 'Lemma #[w:"honor" l:"en"]) 'has honorEnN
    link (is 'Lemma #[w:"honor" l:"es"]) 'has honorEsN
    link (is 'Lemma #[w:"dishonor" l:"en"]) 'has dishonorEnN

    link honorEnN 'means tokenS
    link honorEsN 'means tokenS

    link dishonorEnN 'antonym honorEnN
]]